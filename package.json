    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
  },
  "dependencies": {
    "aws-sdk": "^2.403.0",
    "axios": "^0.19.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "nodemailer": "^5.1.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "concurrently": "^4.1.0"
  }
}
